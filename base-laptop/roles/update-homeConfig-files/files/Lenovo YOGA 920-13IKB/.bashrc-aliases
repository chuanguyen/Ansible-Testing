alias auracle_update='auracle sync | cut -f1 -d " " | xargs -n 1 auracle clone'
alias auracle_upgrade='ls | xargs -n 1 -I{} sh -c "cd {} && sudo pacman -U *.pkg.tar.xz --noconfirm && cd .."'
alias yay='yay --aur'
alias darktable_backup='tar -czvf ~/smb4k/family-nas-001.local/home/Documents/Backups/DarkTable/darktable_config.tar.gz ~/.config/darktable/'
alias make_ansible='make -f {{ ansible_env.HOME }}/docker-base/ansible/Makefile'